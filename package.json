{
  "name": "unity-command-github-action",
  "version": "1.1.5",
  "description": "This action executes the Unity command line.",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm install && ncc build src/main.ts --license licenses.txt",
    "clean": "rimraf dist && rimraf node_modules && npm cache clean --force",
    "release": "bash ./publish.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akiojin/unity-command-github-action.git"
  },
  "keywords": [
    "unity",
    "unity3d"
  ],
  "author": "Akio Jinsenji <akio-jinsenji@cloud-creative-studios.com> (https://github.com/akiojin)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/akiojin/unity-command-github-action/issues"
  },
  "homepage": "https://github.com/akiojin/unity-command-github-action#readme",
  "devDependencies": {
    "@actions/core": "1.10.1",
    "@actions/exec": "1.1.1",
    "@akiojin/unity-command": "1.2.7",
    "@commitlint/cli": "17.7.2",
    "@commitlint/config-conventional": "17.7.0",
    "@types/node": "20.10.1",
    "@vercel/ncc": "0.36.1",
    "husky": "8.0.3",
    "rimraf": "5.0.5",
    "typescript": "5.2.2"
  },
  "private": true
